{
  "name": "biiiiiigmonster/cache",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "biiiiiigmonster/cache component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "swoft/framework": "~2.0.0",
    "swoft/serialize": "~2.0.0",
    "swoft/redis": "~2.0.0",
    "psr/simple-cache": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "BiiiiiigMonster\\Cache\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "BiiiiiigMonster\\CacheTest\\Unit\\": "test/unit/",
      "BiiiiiigMonster\\CacheTest\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
